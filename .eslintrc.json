{
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "es6": true
    },
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "rules": {
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ]
  }